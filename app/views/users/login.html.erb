<fieldset class="FormCenter">
<h1>Log In</h1>
	<%= form_tag url_for(:controller => 'users', :action => 'login'), :method => 'get' do %>
<table class="App">
			 Password Updated Sucessfully  Please Login <br>
<tr>
<td><%= label_tag :email %></td>
<td><%= text_field_tag :email %></td>
</tr>
<tr>
<td><%= label_tag :password %></td>
<td><%= password_field_tag :password %></td>
</tr>
<tr>
<td><%= submit_tag "Log In" %></td>
</tr>
</table>
 <%end%>
</fieldset>

